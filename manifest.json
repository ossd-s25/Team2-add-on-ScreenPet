{
    "manifest_version": 3,
    "name": "Virtual Pet",
    "version": "1.0",
    "description": "A virtual pet that you can feed!",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "background":{
      "script": ["background.js"]
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    },
    "permissions": ["storage"]
  }
  